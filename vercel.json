{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "YANDEX_DIRECT_TOKEN": "@yandex-direct-token",
    "YANDEX_DIRECT_LOGIN": "@yandex-direct-login"
  }
}
